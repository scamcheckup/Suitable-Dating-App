# Supabase Configuration (Required)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Cloudflare R2 Storage (Required for photo uploads)
EXPO_PUBLIC_R2_ENDPOINT=your_r2_endpoint
EXPO_PUBLIC_R2_BUCKET=your_r2_bucket_name
EXPO_PUBLIC_R2_ACCESS_KEY=your_r2_access_key
EXPO_PUBLIC_R2_SECRET_KEY=your_r2_secret_key

# Kudisms SMS Service (Optional)
EXPO_PUBLIC_KUDISMS_API_KEY=your_kudisms_api_key
EXPO_PUBLIC_KUDISMS_API_ENDPOINT=your_kudisms_endpoint
EXPO_PUBLIC_KUDISMS_SENDER_ID=Suitable

# Gemini AI (Optional - for AI chat features)
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key

# Paystack Payment (Optional - for premium features)
EXPO_PUBLIC_PAYSTACK_PUBLIC_KEY=your_paystack_public_key
EXPO_PUBLIC_PAYSTACK_SECRET_KEY=your_paystack_secret_key

# Stream Chat (Optional - for real-time chat)
EXPO_PUBLIC_STREAM_API_KEY=your_stream_api_key
EXPO_PUBLIC_STREAM_SECRET=your_stream_secret